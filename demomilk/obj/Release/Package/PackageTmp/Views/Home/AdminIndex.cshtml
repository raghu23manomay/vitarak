
@model  IEnumerable<demomilk.Models.RouteDetails>

@using PagedList;
@using PagedList.Mvc;
@using GridMvc.Html
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>

<div class="container-fluid animatedParent animateOnce">
    <div class="tab-content my-3" id="v-pills-tabContent">
        <div class="tab-pane animated fadeInUpShort show active go" id="v-pills-all" role="tabpanel" aria-labelledby="v-pills-all-tab">
            <div class="row my-3">
                <div class="col-md-12">
                    <div class="card r-0 shadow">
                        <div class="table-responsive">
                            <form>
                                <table class="table table-striped table-hover r-0">
                                    <thead>
                                        <tr class="no-b">


                                            <th><b style="margin-left:55px;">AERA</b></th>

                                        </tr>
                                    </thead>

                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                @*<td class="hidden">@item.AreaID<td>*@
                                                <td><strong style="margin-left:55px;">@item.Area</strong></td>
                                                <td><a href="@Url.Action("FetchEditArea", "Home",new { @AreaId = item.AreaID})" data-toggle="modal" data-target="#mdlEditRoute"><i class="icon-pencil"></i></a></td>
                                            </tr>

                                        }

                                    </tbody>
                                </table>
                            </form>
                        </div>
                    </div>
                </div>
            </div>




            <nav class="my-3" aria-label="Page navigation">
                @Html.PagedListPager((IPagedList)Model, page => Url.Action("index", "Home", new { page }), PagedListRenderOptions.EnableUnobtrusiveAjaxReplacing(new PagedListRenderOptions() { MaximumPageNumbersToDisplay = 4, DisplayPageCountAndCurrentLocation = true }, new AjaxOptions() { HttpMethod = "POST", UpdateTargetId = "mgrid", OnComplete = "PagedOnComplete" }))
                @if (Model.Count() == 0)
                {
                    <ul class="pagination"></ul>
                }
            </nav>
